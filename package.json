{
  "name": "Sliding-Regression-Tool",
  "version": "0.0.1",
  "author": "Michael Whitmer, Preston Shires, Michael Kaminski",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "clean": "rm -rf node_modules && rm -rf dist && rm -f package-lock.json",
    "electron": "electron .",
    "build-watch": "ng build --op dist -w",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win32": "electron-packager .  --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Sliding Regression Tool\"",
    "package-win64": "electron-packager .  --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Sliding Regression Tool\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@types/lodash": "^4.14.104",
    "bootstrap": "4.0.0",
    "core-js": "^2.4.1",
    "d3": "^4.13.0",
    "excel-as-json": "^2.0.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "less": "^3.0.1",
    "ng2-bootstrap-modal": "^1.0.1",
    "ng2-toasty": "^4.0.2",
    "rxjs": "^5.5.6",
    "xls-to-json": "^0.4.0",
    "xlsx": "^0.12.5",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "electron": "^1.7.6",
    "electron-packager": "^9.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "^2.5.3"
  },
  "description": "ENPI tool with regression capabilities in the form of a desktop application.",
  "repository": "https://github.com/ORNL-AMO/Sliding-Regression-Tool",
  "keywords": [
    "Electron",
    "ENPI",
    "Desktop",
    "ORNL",
    "DOE"
  ],
  "bugs": {
    "url": "https://github.com/ORNL-AMO/Sliding-Regression-Tool/issues"
  },
  "homepage": "https://github.com/ORNL-AMO/Sliding-Regression-Tool#readme"
}
